{
  "name": "kurukshetra",
  "version": "2.0.0",
  "description": "Professional OWASP Top 10 Cybersecurity Training Platform - Intentionally Vulnerable Web Application for Ethical Hacking Practice",
  "author": "Kurukshetra Security Team",
  "keywords": [
    "cybersecurity",
    "owasp",
    "penetration-testing",
    "vulnerability-assessment",
    "ethical-hacking",
    "security-training"
  ],
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "npm run seed:auto && next dev --turbopack -p 9002",
    "build": "npm run seed:auto && next build",
    "start": "next start --port 9002",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "npm run typecheck && npm run lint && npm run test:security",
    "test:security": "node scripts/vulnerability-tests.js",
    "test:endpoints": "node scripts/endpoint-tests.js",
    "test:database": "node scripts/database-connection-test.js",
    "test:all": "npm run test:database && npm run test:security && npm run test:endpoints",
    "deploy": "npm run build && npm run start",
    "deploy:render": "npm run build",
    "setup:sqlite": "echo 'Setting up SQLite database...' && DB_TYPE=sqlite npm run dev",
    "setup:mongodb": "echo 'Setting up MongoDB database...' && DB_TYPE=mongo npm run dev",
    "seed:auto": "node scripts/seed-database.js",
    "seed:force": "node scripts/seed-database.js --force",
    "reset:db": "node scripts/reset-database.js",
    "cleanup:db": "node scripts/cleanup-database.js"
  },
  "dependencies": {
    "@genkit-ai/googleai": "^1.13.0",
    "@genkit-ai/next": "^1.13.0",
    "@hookform/resolvers": "^4.1.3",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@types/bcrypt": "^6.0.0",
    "bcrypt": "^6.0.0",
    "body-parser": "1.18.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.6.1",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^12.23.6",
    "genkit": "^1.13.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.475.0",
    "mongoose": "^8.16.5",
    "next": "15.3.3",
    "patch-package": "^8.0.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwind-merge": "^3.0.1",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "eslint": "9.31.0",
    "eslint-config-next": "15.4.2",
    "genkit-cli": "^1.13.0",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "5.8.3"
  }
}
